{
  "name": "spearforestlauncher",
  "version": "0.0.1",
  "productName": "Spearforest Launcher",
  "description": "Modded Minecraft Launcher",
  "author": {
    "name": "Sunghyun Kim",
    "email": "hello@sunghyun.me"
  },
  "license": "UNLICENSED",
  "homepage": "https://github.com/Kevin-Studio-Dev/SpearfishForest4-HeliosLauncher",
  "bugs": {
    "url": "https://github.com/Kevin-Studio-Dev/SpearfishForest4-HeliosLauncher/issues"
  },
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build",
    "dist": "electron-builder",
    "dist:win": "electron-builder --win --publish always",
    "dist:mac": "electron-builder --mac --publish always",
    "dist:linux": "electron-builder --linux --publish always",
    "lint": "eslint --config .eslintrc.json .",
    "publish": "electron-builder -p always"
  },
  "engines": {
    "node": "20.x.x"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "adm-zip": "^0.5.16",
    "discord-rpc-patch": "^4.0.1",
    "ejs": "^3.1.10",
    "ejs-electron": "^3.0.0",
    "electron-updater": "^6.3.9",
    "fs-extra": "^11.1.1",
    "github-syntax-dark": "^0.5.0",
    "got": "^11.8.5",
    "helios-core": "~2.2.3",
    "helios-distribution-types": "^1.3.0",
    "jquery": "^3.7.1",
    "lodash.merge": "^4.6.2",
    "semver": "^7.6.3",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "electron": "33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^8.57.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kevin-Studio-Dev/SpearfishForest4-HeliosLauncher.git"
  },
  "build": {
    "appId": "com.spearfish.launcher",
    "productName": "SpearfishLauncher",
    "artifactName": "${productName}-setup-${version}.${ext}",
    "electronDownload": {
      "isVerifyChecksum": false
    },
    "publish": [{
      "provider": "github",
      "owner": "Kevin-Studio-Dev",
      "repo": "SpearfishForest4-HeliosLauncher"
    }],
    "mac": {
      "category": "public.app-category.games",
      "target": [
        "dmg",
        "zip"
      ],
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "electronLanguages": ["en"],
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "forceCodeSigning": false,
      "icon": null
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/icon.png",
      "maintainer": "Sunghyun Kim <hellosunghyun@gmail.com>",
      "category": "Game"
    }
  }
}
